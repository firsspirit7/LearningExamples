<apex:page controller="DynamicObjectsInsertMode">
  <apex:form >
    <apex:pageBlock >
      <apex:pageMessage severity="warning" strength="3" rendered="{!isdisplay}"/>
      <apex:pageMessages />
        <div align ="center" style="font-size: 26px;color: red;"> 
           <apex:outputText >Enter Object Name</apex:outputText>&nbsp;&nbsp; 
           <apex:inputText value="{!objName}"/>&nbsp;&nbsp; 
           <apex:commandButton value="Go" action="{!go}" style="width: 50px;"/>  
      </div>               
    </apex:pageBlock>
  </apex:form> 
</apex:page>